{
  "name": "stays-platform",
  "version": "1.0.0",
  "description": "back end for stays app",
  "main": "build/src/server.js",
  "scripts": {
    "dev": "concurrently \"npm run build:ts -- -w\" \"nodemon --config tsoa-spec-and-routes.nodemon.json\" \"nodemon --config nodemon.json\"",
    "build:ts": "tsc --outDir build --experimentalDecorators",
    "build:spec-and-routes": "tsoa spec-and-routes",
    "build": "npm run build:spec-and-routes && npm run build:ts",
    "start": "node build/stays-platform/src/server.js",
    "lint": "eslint --fix --ext .ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "author": "Nick Schrock",
  "license": "ISC",
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.8",
    "@types/node-geocoder": "^3.24.4",
    "airtable": "^0.11.1",
    "axios": "^0.25.0",
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "express": "^4.17.2",
    "file-class": "^0.1.3",
    "filereader": "^0.10.3",
    "firebase": "^9.6.3",
    "firebase-admin": "^10.0.1",
    "lodash": "^4.17.21",
    "node-geocoder": "^3.28.0",
    "ow": "^0.28.1",
    "require": "^2.4.20",
    "tsoa": "^3.14.1",
    "typescript": "^4.5.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/lodash": "^4.14.178",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.10.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "undefined": "^0.1.0"
  }
}
